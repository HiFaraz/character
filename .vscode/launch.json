  "version": "0.2.0",
  "configurations": [{
    "request": "launch",
    "type": "node",
    "name": "Run tests",
    "program": "${workspaceRoot}\\node_modules\\mocha\\bin\\_mocha",
    "stopOnEntry": false,
    "args": ["lib/**/*.spec.js", "lib/test"],
    "env": {
      "DEBUG":"identity-desk*"
    },
    "runtimeExecutable": null,
    "sourceMaps": true,
    "outFiles": ["${workspaceRoot}\\lib\\**\\*.js"],
    "smartStep": true
  }]
}