{
  "name": "identity-desk",
  "version": "0.0.0",
  "description": "The Node.js identity solution",
  "main": "lib",
  "scripts": {
    "prebuild": "npm outdated && npm run lint && npm run clean-build",
    "build": "npm run build:src",
    "build-ci": "npm run build:src",
    "build-watch": "npm run build -- -- --watch",
    "build:src": "babel src --out-dir lib --copy-files --source-maps inline",
    "check-security": "nsp check",
    "clean-build": "rimraf lib",
    "prelint": "npm run check-security",
    "lint": "npm run lint-ci -- --fix",
    "lint-ci": "eslint --format=table .",
    "test": "mocha lib/**/*.spec.js lib/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiFaraz/identity-desk.git"
  },
  "keywords": [
    "auth",
    "identity",
    "sso",
    "authentication",
    "authorization",
    "access",
    "control"
  ],
  "author": "Faraz Syed <hello@farazsyed.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HiFaraz/identity-desk/issues"
  },
  "homepage": "https://github.com/HiFaraz/identity-desk#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "body-parser": "^1.17.2",
    "connect-session-sequelize": "^4.1.0",
    "eslint": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "identity-desk-local": "^0.1.0",
    "mocha": "^3.4.2",
    "nsp": "^2.6.3",
    "rimraf": "^2.6.1",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "debug": "^2.6.8",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-generic-session": "^1.11.6",
    "koa-generic-session-sequelize": "^1.0.9",
    "koa-is-json": "^1.0.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.2.0",
    "lodash": "^4.17.4",
    "on-finished": "^2.3.0",
    "read-data": "^1.1.0",
    "semver": "^5.3.0",
    "sequelize": "^4.0.0",
    "source-map-support": "^0.4.15",
    "statuses": "^1.3.1"
  }
}
