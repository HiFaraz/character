{
  "name": "character",
  "version": "0.0.0",
  "description": "Authentication, SSO, user management, and overall identity solution for Node.js",
  "main": "lib",
  "scripts": {
    "prebuild": "nsp check && npm run lint && rimraf lib",
    "build": "babel src --out-dir lib --copy-files --source-maps inline",
    "build-watch": "npm run build -- --watch",
    "lint": "eslint --format=table .",
    "prepublish": "npm run build && npm test",
    "test": "cross-env DEBUG=character* mocha -r lib/entry lib/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiFaraz/character.git"
  },
  "keywords": [
    "auth",
    "identity",
    "sso",
    "authentication",
    "authorization",
    "access",
    "control"
  ],
  "author": "Faraz Syed <hello@farazsyed.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HiFaraz/character/issues"
  },
  "homepage": "http://characterjs.com/",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.1",
    "depcheck": "^0.6.8",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express": "^4.0.0",
    "mocha": "^4.0.1",
    "nsp": "^2.8.1",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "capitalize": "^1.0.0",
    "debug": "^3.0.0",
    "lodash": "^4.0.0",
    "read-data": "^1.1.0",
    "source-map-support": "^0.5.0"
  },
  "peerDependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.0.0",
    "sequelize": "^4.0.0"
  }
}