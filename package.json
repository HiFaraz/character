{
  "name": "identity-desk",
  "version": "0.0.0",
  "description": "Authentication, SSO, user management, and overall identity solution for Node.js",
  "main": "lib",
  "scripts": {
    "prebuild": "npm run lint && npm run clean-build",
    "build": "npm run build:src",
    "build-ci": "npm run build:src",
    "build-watch": "npm run build -- -- --watch",
    "build:src": "babel src --out-dir lib --copy-files --source-maps inline",
    "check-security": "nsp check",
    "clean-build": "rimraf lib",
    "lint": "npm run lint-ci -- --fix",
    "lint-ci": "eslint --format=table .",
    "test": "npm run unit-tests && npm run integration-tests",
    "integration-tests": "mocha lib/test",
    "unit-tests": "mocha lib/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HiFaraz/identity-desk.git"
  },
  "keywords": [
    "auth",
    "identity",
    "sso",
    "authentication",
    "authorization",
    "access",
    "control"
  ],
  "author": "Faraz Syed <hello@farazsyed.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HiFaraz/identity-desk/issues"
  },
  "homepage": "http://identitydesk.io/",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "body-parser": "^1.17.2",
    "connect-session-sequelize": "^4.1.0",
    "eslint": "^4.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-prettier": "^2.1.2",
    "express": "^4.15.3",
    "express-session": "^1.15.5",
    "identity-desk-local": "^0.1.0",
    "mocha": "^3.4.2",
    "nsp": "^2.6.3",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "sqlite3": "^3.1.8",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "arrify": "^1.0.1",
    "babel-polyfill": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "capitalize": "^1.0.0",
    "debug": "^3.0.0",
    "http-codes": "^1.0.0",
    "lodash": "^4.17.4",
    "read-data": "^1.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "semver": "^5.3.0",
    "sequelize": "^4.0.0",
    "source-map-support": "^0.4.15"
  },
  "peerDependencies": {
    "body-parser": "^1.17.2",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.15.3",
    "express-session": "^1.15.5"
  }
}
